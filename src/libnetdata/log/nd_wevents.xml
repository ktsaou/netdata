                    <channel name="Netdata/Alerts" type="Admin" symbol="NetdataAlertsChannel" enabled="true" isolation="Application" access="O:BAG:SYD:(A;;0xf0007;;;WD)"/>
                    <channel name="Netdata/ACLK" type="Debug" symbol="NetdataACLKChannel" enabled="true" isolation="Application" access="O:BAG:SYD:(A;;0xf0007;;;WD)"/>
                </channels>

                <!-- Define the events -->
                <events>
                    <!-- Daemon Events -->
                    <event symbol="DaemonInfo" value="0x1000" channel="Netdata/Operational" level="win:Informational" task="LogEvent" opcode="Info" template="NetdataLogTemplate"/>
                    <event symbol="DaemonWarning" value="0x1001" channel="Netdata/Operational" level="win:Warning" task="LogEvent" opcode="Warning" template="NetdataLogTemplate"/>
                    <event symbol="DaemonError" value="0x1002" channel="Netdata/Operational" level="win:Error" task="LogEvent" opcode="Error" template="NetdataLogTemplate"/>

                    <!-- Collector Events -->
                    <event symbol="CollectorInfo" value="0x2000" channel="Netdata/Collectors" level="win:Informational" task="LogEvent" opcode="Info" template="NetdataLogTemplate"/>
                    <event symbol="CollectorWarning" value="0x2001" channel="Netdata/Collectors" level="win:Warning" task="LogEvent" opcode="Warning" template="NetdataLogTemplate"/>
                    <event symbol="CollectorError" value="0x2002" channel="Netdata/Collectors" level="win:Error" task="LogEvent" opcode="Error" template="NetdataLogTemplate"/>

                    <!-- Access Events -->
                    <event symbol="AccessInfo" value="0x3000" channel="Netdata/Access" level="win:Informational" task="LogEvent" opcode="Info" template="NetdataLogTemplate"/>
                    <event symbol="AccessWarning" value="0x3001" channel="Netdata/Access" level="win:Warning" task="LogEvent" opcode="Warning" template="NetdataLogTemplate"/>
                    <event symbol="AccessError" value="0x3002" channel="Netdata/Access" level="win:Error" task="LogEvent" opcode="Error" template="NetdataLogTemplate"/>

                    <!-- Health Events -->
                    <event symbol="HealthInfo" value="0x4000" channel="Netdata/Alerts" level="win:Informational" task="LogEvent" opcode="Info" template="NetdataLogTemplate"/>
                    <event symbol="HealthWarning" value="0x4001" channel="Netdata/Alerts" level="win:Warning" task="LogEvent" opcode="Warning" template="NetdataLogTemplate"/>
                    <event symbol="HealthError" value="0x4002" channel="Netdata/Alerts" level="win:Error" task="LogEvent" opcode="Error" template="NetdataLogTemplate"/>

                    <!-- ACLK Events -->
                    <event symbol="ACLKInfo" value="0x5000" channel="Netdata/ACLK" level="win:Informational" task="LogEvent" opcode="Info" template="NetdataLogTemplate"/>
                    <event symbol="ACLKWarning" value="0x5001" channel="Netdata/ACLK" level="win:Warning" task="LogEvent" opcode="Warning" template="NetdataLogTemplate"/>
                    <event symbol="ACLKError" value="0x5002" channel="Netdata/ACLK" level="win:Error" task="LogEvent" opcode="Error" template="NetdataLogTemplate"/>
                </events>

