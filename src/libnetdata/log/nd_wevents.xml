                      symbol="Netdata"
                      messageFileName="C:\Windows\System32\nd_wevents.dll"
                      resourceFileName="C:\Windows\System32\nd_wevents.dll"
                      parameterFileName="C:\Windows\System32\nd_wevents.dll">

                <!-- Define the channels -->
                <channels>
                    <channel name="Netdata/Operational" type="Operational" symbol="NetdataOperationalChannel" enabled="true" isolation="Application" access="O:BAG:SYD:(A;;0xf0007;;;WD)"/>
                    <channel name="Netdata/Collectors" type="Operational" symbol="NetdataCollectorsChannel" enabled="true" isolation="Application" access="O:BAG:SYD:(A;;0xf0007;;;WD)"/>
